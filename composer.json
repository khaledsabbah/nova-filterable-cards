{
    "name": "khaledgamal/nova-filterable-cards",
    "description": "Filterable Metric Cards for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
    },
    "autoload": {
        "psr-4": {
            "Beyondcode\\FilterableCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beyondcode\\FilterableCard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beyondcode\\FilterableCard\\CardServiceProvider"
            ]
        }
    },
    "repositories": [],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "mockery/mockery": "^1.1"
    }
}
